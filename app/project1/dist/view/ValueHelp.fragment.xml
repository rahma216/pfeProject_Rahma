<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog1"
		title="Entities"
		items="{mainModel>/Entity}"
		search=".onValueHelpDialogSearch1"
		confirm=".onValueHelpDialogClose1"
		cancel=".onValueHelpDialogClose1">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{mainModel>name}"
			description="{mainModel>name}" />
	</SelectDialog>
</core:FragmentDefinition>